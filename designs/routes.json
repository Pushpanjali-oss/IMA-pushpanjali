{
    "routes": [
        {
            "path": "incidentpage",
            "component": "incidentmainComponent",
            "canActivate": "NeutrinosAuthGuardService"
        },
        {
            "path": "incidentdetails",
            "component": "incidentdetailsComponent",
            "canActivate": "NeutrinosAuthGuardService"
        },
        {
            "path": "loginpage",
            "component": "loginpageComponent"
        },
        {
            "path": "profile",
            "component": "profilepageComponent"
        },
        {
            "path": "''",
            "redirectTo": "incidentpage",
            "pathMatch": "full",
            "default": true
        },
        {
            "path": "**",
            "component": "PageNotFoundComponent"
        }
    ]
}
